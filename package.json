{
  "name": "@pixi/tilemap",
  "version": "3.2.0",
  "description": "A low-level, optimized rectangular tilemap implementation for PixiJS.",
  "author": "Ivan Popelyshev",
  "contributors": [
    "Ivan Popelyshev <ivan.popelyshev@gmail.com>",
    "Shukant Pal <shukant@pixijs.io>"
  ],
  "main": "./dist/pixi-tilemap.js",
  "module": "./lib/pixi-tilemap.es.js",
  "bundle": "./dist/pixi-tilemap.umd.js",
  "namespace": "PIXI.tilemap",
  "types": "./index.d.ts",
  "homepage": "http://www.rpgmakerweb.com/",
  "bugs": "https://github.com/pixijs/pixi-tilemap/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/pixi-tilemap.git"
  },
  "scripts": {
    "cleanup": "rimraf dist && mkdirp dist && rimraf lib && mkdirp lib && rimraf compile",
    "prestart": "yarn cleanup",
    "start": "parallelshell \"yarn watch\"",
    "watch": "tsc -w",
    "prebuild": "yarn cleanup",
    "build": "rollup -c node_modules/@pixi-build-tools/rollup-configurator/index.js && yarn build:types",
    "build:types": "rimraf ./compile && tsc -p ./tsconfig.compile.json && api-extractor run",
    "docs": "typedoc --out ./docs/ --readme README.md ./src/",
    "lint": "eslint ./src",
    "unit-test": "floss --path test --"
  },
  "files": [
    "dist/",
    "lib/",
    "index.d.ts",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "peerDependencies": {
    "@pixi/canvas-renderer": "^6.0.0-rc.3",
    "@pixi/constants": "^6.0.0-rc.3",
    "@pixi/core": "^6.0.0-rc.3",
    "@pixi/display": "^6.0.0-rc.3",
    "@pixi/graphics": "^6.0.0-rc.3",
    "@pixi/math": "^6.0.0-rc.3",
    "@pixi/utils": "^6.0.0-rc.3"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.9.4",
    "@pixi-build-tools/rollup-configurator": "~1.0.11",
    "@pixi/canvas-renderer": "^6.0.0-rc.3",
    "@pixi/constants": "^6.0.0-rc.3",
    "@pixi/core": "^6.0.0-rc.3",
    "@pixi/display": "^6.0.0-rc.3",
    "@pixi/eslint-config": "^2.0.1",
    "@pixi/extract": "^6.0.0-rc.3",
    "@pixi/graphics": "^6.0.0-rc.3",
    "@pixi/math": "^6.0.0-rc.3",
    "@pixi/utils": "^6.0.0-rc.3",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/eventemitter3": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "chai": "^4.3.3",
    "del": "^2.2.0",
    "electron": "^9.4.0",
    "eslint": "^7.21.0",
    "floss": "^3.0.0",
    "jsdoc": "^3.4.0",
    "mkdirp": "^0.5.1",
    "parallelshell": "^2.0.0",
    "relative-deps": "^1.0.6",
    "rimraf": "^2.5.3",
    "rollup": "^2.23.1",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  }
}
